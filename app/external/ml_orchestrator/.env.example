# LLM backend: transformers | openai
LLM_BACKEND=transformers
# Qwen модель для transformers
QWEN_MODEL=Qwen/Qwen2-7B-Instruct
QWEN_DEVICE=auto
QWEN_MAX_NEW_TOKENS=512
QWEN_TEMPERATURE=0.2

# OpenAI-compatible (например, локальный сервер)
OPENAI_API_BASE=http://localhost:8000/v1
OPENAI_API_KEY=not-needed
OPENAI_MODEL=qwen2-7b-instruct

# Параметры пайплайна
MIN_CLASS_CONFIDENCE=0.7
FAISS_INDEX_PATH=./data/faiss_index.bin
KB_JSONL=./data/kb.jsonl
LOGS_PATH=./logs/logs.jsonl
TOP_K_CONTEXT=5
